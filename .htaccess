<IfModule mod_rewrite.c>
  RewriteEngine On

  # --- Angular SPA under /angular-ar/ ---
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteCond %{REQUEST_URI} ^/angular-ar/
  RewriteRule ^angular-ar/.*$ /angular-ar/index.html [L]
</IfModule>

# --- default page ---
DirectoryIndex login.php
